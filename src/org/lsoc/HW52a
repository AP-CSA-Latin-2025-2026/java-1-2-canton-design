public class HW52a{
  public static void main(string[] args){
    int length = 143; 
    int width = 82;
    int perim = length + length + width + width;
    int area = length * width;
    System.out.println(length);
    System.out.println(width);
    System.out.println(area);
    System.out.println(perim);

  }


}